{
  "name": "@rumsan/ui",
  "version": "0.2.21",
  "type": "module",
  "private": false,
  "description": "A comprehensive React component library built with Radix UI and Tailwind CSS",
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "tailwind",
    "radix-ui",
    "typescript",
    "accessible"
  ],
  "homepage": "https://github.com/rumsan/libraries#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rumsan/libraries.git",
    "directory": "packages/rumsan-ui"
  },
  "license": "MIT",
  "author": "Rumsan Group",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "postcss.config.mjs",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint . --max-warnings 0",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@rumsan/sdk": "workspace:*",
    "@rumsan/shadcn": "workspace:*",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.475.0",
    "react-hook-form": "^7.63.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "glob": "^11.0.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./hooks/*": {
      "import": "./dist/hooks/*.js",
      "types": "./dist/hooks/*.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./providers/*": {
      "import": "./dist/providers/*.js",
      "types": "./dist/providers/*.d.ts"
    },
    "./stores": {
      "import": "./dist/stores/index.js",
      "types": "./dist/stores/index.d.ts"
    },
    "./stores/*": {
      "import": "./dist/stores/*.js",
      "types": "./dist/stores/*.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
